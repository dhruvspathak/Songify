# Songify Frontend Configuration - Staging Environment
# Copy this file to .env.staging for staging deployment
# These variables are safe to expose (no secrets)

# Spotify Client Configuration (Staging)
VITE_CLIENT_ID=your_staging_spotify_client_id

# Application URLs (Staging)
VITE_REDIRECT_URI=https://staging.yourdomain.com/callback
VITE_BACKEND_URL=https://api-staging.yourdomain.com

# Environment
NODE_ENV=staging

# Feature Flags (Optional - more debugging for staging)
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false
